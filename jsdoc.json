{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "recurseDepth": 10,
    "source": {
        "include": [ "app/js/ttth/", "main.js" ],
        "exclude": [  ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "destination": "./docs/jsdocs",
        "recurse": true
    }
}
